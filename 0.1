
class Auto:
  def __init__(self,brand,yearOfMan):
    self.brand = brand
    self.yearOfMan = yearOfMan
    self.dictinary = {'Toyota':'Japan', 'Renault':'France'}

  def show(self):
    print(self.brand,self.yearOfMan)
    
  def showCountry(self):
    print(self.brand,self.yearOfMan,' is from ', self.dictinary.pop(self.brand))

brands =['Fiat','Honda','Toyota','Hyundai','Renault']
yearOfManStart= 1990
